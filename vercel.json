{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.js?path=:path*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
